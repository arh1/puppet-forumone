Host <%= @title %>
  HostName <%= @hostname %>
  Port <%= @port %>
  User <%= @remote_user %>
<% if @proxy_command -%>
  ProxyCommand <%= @proxy_command %>
<% end %>
<% if @connect_timeout -%>
  ConnectTimeout <%=  @connect_timeout %>
<% end %>
<% if @forward_agent -%>
  ForwardAgent <%= @forward_agent -%>
<% end %>